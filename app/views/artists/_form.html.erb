<%#Currently this for is invalid as only admin can create new artists. A new model Artist will be worked on soon%>
<%= form_for @artist do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :image %>
    <%= f.text_field :image %>
    <%= f.label :social %>
    <%= f.text_field :social %>
    <%= f.label :work %>
    <%= f.text_field :work %>
    <%= f.label :bio %>
    <%= f.text_field :bio %>
    <%= f.submit %>
<% end %>