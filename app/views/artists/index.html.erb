<h1>All Artists</h1>

<% @artists.each do |artist|%>
    <%= artist_tag artist.image, :class => 'thumb' if artist.image.present? %>
    <ul>
        <li>Name: <%= link_to artist.name, artist_new_path %></li>
        <li>Social: <%= artist.social %></li>
        <li>Work: <%= artist.work %></li>
        <li>Bio: <%= artist.bio %></li>
    </ul>
<% end%>
<% end %> 


